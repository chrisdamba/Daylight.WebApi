<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <!--<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">-->

    <title>@(ViewBag.Title ?? "Shekhinah Medical Practice - SmartAdmin")</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- http://davidbcalhoun.com/2010/viewport-metatag -->
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <!-- Single CSS files -->
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap.min.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/font-awesome.min.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/jarvis-widgets.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smart-forms.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/x-editable.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/fullcalendar.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/colorpicker.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/backbone.autocomplete.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-timepicker.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/animated.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/summernote.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-markdown.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/dropzone/basic.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/dropzone/dropzone.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/pricing-table.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/superbox.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-tagsinput.css")">

    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/DT_bootstrap.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/TableTools.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/fd-slider.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-slider.css")">

    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/jquery-ui-1.10.3.custom.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smart-chat.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/notifications.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/colorpallet.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/main.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smartadmin-production_unminified.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/elements.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/error404.css")">

    <!-- patients list page specific styles -->
    <link rel="stylesheet" href="@Url.Content("~/css/compiled/user-list.css")" type="text/css" media="screen" />


    <!-- FAVICONS -->
    <link rel="shortcut icon" href="@Url.Content("~/img/favicon/favicon.ico")" type="image/x-icon">
    <link rel="icon" href="@Url.Content("~/img/favicon/favicon.ico")" type="image/x-icon">

    <!-- GOOGLE FONT -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,300,400,700">

</head>
<body class="no-right-panel">
    <!-- possible classes: minified, no-right-panel, fixed-ribbon, fixed-header, fixed-width-->
    <header id="header">
        <!--<span id="logo"></span>-->
        <div id="logo-group">
            <span id="logo">
                <img src="@Url.Content("~/img/logo.png")" alt="SmartAdmin">
            </span>
            <span id="activity" class="activity-dropdown">
                <i class="fa fa-user"></i>
                <b class="badge">
                    21
                </b>
            </span>
            <div class="ajax-dropdown">

                <div class="btn-group btn-group-justified" data-toggle="buttons">
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifymail"> Msgs (14)
                    </label>
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifynotifications"> notify (3)
                    </label>
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifytasks"> Tasks (4)
                    </label>
                </div>

                <div class="ajax-notifications custom-scroll">
                    <div class="alert alert-transparent">
                        <h4>Click a button to show messages here</h4>
                        This blank page message helps protect your privacy, or you can show the first message here automatically.
                    </div>

                    <i class="fa fa-lock fa-4x fa-border"></i>

                </div>

                <span>
                    Last updated on: 12/12/2013 9:43AM
                    <button type="button" data-loading-text="
                        <i class='fa fa-refresh fa-spin'></i> Loading..." class="btn btn-xs btn-default pull-right">
                        <i class="fa fa-refresh"></i>
                    </button>
                </span>
            </div>
            <!-- END AJAX-DROPDOWN -->
        </div>


        <div class="pull-right">
            <div id="hide-menu" class="btn-header pull-right">
                <span>
                    <a href="javascript:void(0);"><i class="fa fa-reorder"></i></a>
                </span>
            </div>
            <div id="logout" class="btn-header transparent pull-right">
                <span>
                    <a href="~/Home/Logout/"><i class="fa fa-sign-out"></i></a>
                </span>
            </div>
            <div id="search-mobile" class="btn-header transparent pull-right">
                <span>
                    <a href="javascript:void(0)"><i class="fa fa-search"></i></a>
                </span>
            </div>

            <form action="#ajax/search.html" class="header-search pull-right">
                <input type="text" placeholder="Search for a patient" id="search-fld">
                <button type="submit">
                    <i class="fa fa-search"></i>
                </button>
                <a href="javascript:void(0);" id="cancel-search-js"><i class="fa fa-times"></i></a>
            </form>

            <!-- This right panel will be released in the future version of SmartAdmin
                <div id="right-panel-overlap-btn" class="btn-header transparent pull-right">
                <span>
                    <a href="javascript:void(0);" rel="tooltip" title="Show Right Panel" data-placement="bottom"><i class="fa fa-long-arrow-left"></i></a>
                </span>
            </div>

            <ul class="header-dropdown-list hidden-xs">
                <li>
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <img alt="" src="img/flags/us.png">
                        <span>
                            US
                        </span>
                        <i class="fa fa-angle-down"></i>
                    </a>
                    <ul class="dropdown-menu pull-right">
                        <li class="active"><a href="javascript:void(0);"><img alt="" src="img/flags/us.png"> US</a></li>
                        <li><a href="javascript:void(0);"><img alt="" src="img/flags/es.png"> Spanish</a></li>
                        <li><a href="javascript:void(0);"><img alt="" src="img/flags/de.png"> German</a></li>
                    </ul>
                </li>
            </ul>-->




        </div>
    </header>
    <aside id="left-panel">
        <div class="login-info">
            <span>
                <img src="@Url.Content("~/img/avatars/sunny.png")" alt="me" class="online" />
                <a href="javascript:void(0);" id="show-shortcut">Ian Muringai <i class="fa fa-angle-down"></i></a>
            </span>
        </div>
        <nav>
            <ul>
                <li class=""><a href="/" title="Dashboard"><i class="fa fa-lg fa-fw fa-dashboard"></i>  <span class="menu-item-parent">Dashboard</span></a></li>
                <li><a href="#"><i class="fa fa-lg fa-fw fa-inbox"></i>  <span class="menu-item-parent">Messages</span><span class="badge pull-right inbox-badge">14</span></a></li>
                <li>
                    <a href="#view/patients"><i class="fa fa-lg fa-fw fa-group"></i>  <span class="menu-item-parent">My Patients</span></a>
                    <ul>
                        <li><a href="#view/patients?filter=adults">Adults (> 18)</a></li>
                        <li><a href="#view/patients?filter=children">Children (< 18)</a></li>
                        <li><a href="#view/patients?filter=removed">Removed</a></li>
                        <li><a href="#view/patients?filter=recent">Recently Added</a></li>
                    </ul>
                </li>
                <li>
                    <a href="#"><i class="fa fa-lg fa-fw fa-bar-chart-o"></i>  <span class="menu-item-parent">Reports</span></a>
                    <ul>
                        <li><a href="#">Patients Chart</a></li>
                        <li><a href="#">Procedures Charts</a></li>
                        <li><a href="#">Medications Charts</a></li>
                    </ul>
                </li>
                <li>
                    <a href="#"><i class="fa fa-lg fa-fw fa-file-text-o"></i>  <span class="menu-item-parent">Consultations</span></a>
                    <ul>
                        <li><a href="#">Children</a></li>
                        <li><a href="#">Adults</a></li>
                    </ul>
                </li>
                <li><a href="#appointments"><i class="fa fa-lg fa-fw fa-calendar"><em>3</em></i>  <span class="menu-item-parent">Appointments</span></a></li>
            </ul>
        </nav>
        <span class="minifyme">
            <i class="fa fa-arrow-circle-left hit"></i>
        </span>


    </aside>
    <aside id="right-panel"></aside>

    <div id="main" role="main">
        <div id="ribbon">

            <span class="ribbon-button-alignment">
                <span id="refresh" class="btn btn-ribbon" data-title="refresh" rel="tooltip" data-placement="bottom" data-original-title="<i class='text-warning fa fa-warning'></i> Warning! This will reset all your widget settings." data-html="true"><i class="fa fa-refresh"></i></span>
            </span>

            <ol class="breadcrumb">
                <!-- This is auto generated -->
            </ol>
        </div>

        @RenderBody()
    </div>
    <div id="shortcut">
        <ul>
            <li>
                <a href="#ajax/inbox.html" class="jarvismetro-tile big-cubes bg-color-blue">
                    <span class="iconbox">
                        <i class="fa fa-envelope fa-4x"></i>
                        <span>Mail <span class="label pull-right bg-color-darken">14</span></span>
                    </span>
                </a>
            </li>
            <li>
                <a href="#ajax/calendar.html" class="jarvismetro-tile big-cubes bg-color-orangeDark">
                    <span class="iconbox">
                        <i class="fa fa-calendar fa-4x"></i>
                        <span>Calendar</span>
                    </span>
                </a>
            </li>
            <li>
                <a href="#ajax/gmap-xml.html" class="jarvismetro-tile big-cubes bg-color-purple">
                    <span class="iconbox">
                        <i class="fa fa-map-marker fa-4x"></i>
                        <span>Maps</span>
                    </span>
                </a>
            </li>
            <li>
                <a href="#ajax/invoice.html" class="jarvismetro-tile big-cubes bg-color-blueDark">
                    <span class="iconbox">
                        <i class="fa fa-book fa-4x"></i>
                        <span>Invoice <span class="label pull-right bg-color-darken">99</span></span>
                    </span>
                </a>
            </li>
            <li>
                <a href="#ajax/gallery.html" class="jarvismetro-tile big-cubes bg-color-greenLight">
                    <span class="iconbox">
                        <i class="fa fa-picture-o fa-4x"></i>
                        <span>Gallery </span>
                    </span>
                </a>
            </li>
            <li>
                <a href="javascript:void(0);" class="jarvismetro-tile big-cubes selected bg-color-pinkDark">
                    <span class="iconbox">
                        <i class="fa fa-user fa-4x"></i>
                        <span>My Profile </span>
                    </span>
                </a>
            </li>
        </ul>
    </div>
    @RenderSection("Javascript", false)
</body>
</html>
