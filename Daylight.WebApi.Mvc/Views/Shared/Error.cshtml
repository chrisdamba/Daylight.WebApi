<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <!--<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">-->

    <title>@(ViewBag.Title ?? "Shekhinah Medical Practice - SmartAdmin")</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- http://davidbcalhoun.com/2010/viewport-metatag -->
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <!-- Single CSS files -->
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap.min.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/font-awesome.min.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/jarvis-widgets.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smart-forms.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/x-editable.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/fullcalendar.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/colorpicker.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/backbone.autocomplete.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-timepicker.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/animated.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/summernote.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-markdown.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/dropzone/basic.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/dropzone/dropzone.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/pricing-table.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/superbox.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-tagsinput.css")">

    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/DT_bootstrap.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/TableTools.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/fd-slider.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/bootstrap-slider.css")">

    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/jquery-ui-1.10.3.custom.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smart-chat.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/notifications.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/colorpallet.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/main.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/smartadmin-production_unminified.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/elements.css")">
    <link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/css/error404.css")">

    <!-- patients list page specific styles -->
    <link rel="stylesheet" href="@Url.Content("~/css/compiled/user-list.css")" type="text/css" media="screen" />


    <!-- FAVICONS -->
    <link rel="shortcut icon" href="@Url.Content("~/img/favicon/favicon.ico")" type="image/x-icon">
    <link rel="icon" href="@Url.Content("~/img/favicon/favicon.ico")" type="image/x-icon">

    <!-- GOOGLE FONT -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,300,400,700">

</head>
<body class="no-right-panel">
    <!-- possible classes: minified, no-right-panel, fixed-ribbon, fixed-header, fixed-width-->
    <header id="header">
        <!--<span id="logo"></span>-->
        <div id="logo-group">
            <span id="logo">
                <img src="@Url.Content("~/img/logo.png")" alt="SmartAdmin">
            </span>
            
            <div class="ajax-dropdown">

                <div class="btn-group btn-group-justified" data-toggle="buttons">
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifymail"> Msgs (14)
                    </label>
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifynotifications"> notify (3)
                    </label>
                    <label class="btn btn-default">
                        <input type="radio" name="activity" id="notifytasks"> Tasks (4)
                    </label>
                </div>

                <div class="ajax-notifications custom-scroll">
                    <div class="alert alert-transparent">
                        <h4>Click a button to show messages here</h4>
                        This blank page message helps protect your privacy, or you can show the first message here automatically.
                    </div>

                    <i class="fa fa-lock fa-4x fa-border"></i>

                </div>

                <span>
                    Last updated on: 12/12/2013 9:43AM
                    <button type="button" data-loading-text="
                        <i class='fa fa-refresh fa-spin'></i> Loading..." class="btn btn-xs btn-default pull-right">
                        <i class="fa fa-refresh"></i>
                    </button>
                </span>
            </div>
            <!-- END AJAX-DROPDOWN -->
        </div>


        <div class="pull-right">
            
        </div>
    </header>
    <div id="main" role="main">
        <div id="ribbon">

            <span class="ribbon-button-alignment">
                
            </span>

            <ol class="breadcrumb">
                <!-- This is auto generated -->
            </ol>
        </div>

        @RenderBody()
    </div>
    <div id="shortcut">
       
    </div>
    @RenderSection("Javascript", false)
</body>
</html>
